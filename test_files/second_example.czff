func void Main() {
    var int n = 5;
    var array<int> arr = new int(n)[];
    arr[0] = -1;
    arr[1] = 2;
    arr[2] = arr[0] + arr[1];
    arr[3] = -(arr[0] * arr[1]);
    arr[4] = arr[0] * (arr[1] + arr[2]) + arr[3];
    
    print arr[0];
    print arr[1];
    print arr[2];
    print arr[3];
    print arr[4];

    return;
}
